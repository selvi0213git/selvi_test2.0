/*
* Kboard style+ - eley
*/
.board-tools {
	margin-bottom: 23px;
	padding-left: 9px;
	padding-right: 9px;
	overflow: hidden;
}

.board-tools .search-form {
	float: left;
}

.board-tools .search-form .search-label {
	display: inline-block;
	margin-right: 0.66667em;
	font-family: 'Noto Sans Korean Regular', sans-serif;
	font-weight: 400;
	font-size: 1.125em;
	vertical-align: middle;
}

.board-tools .search-form .search-group {
	display: inline-block;
	width: 22.5em;
	overflow: hidden;
	vertical-align: middle;
}

.board-tools .search-form .search-group .input-search {
	float: left;
	font-size: 1.125em;
	width: 79.72222%;
}

.board-tools .search-form .search-group .btn-search {
	float: left;
	margin-left: 1.38889%;
	padding-top: 0.27778em;
	padding-bottom: 0.33333em;
	width: 18.88889%;
	background-color: #ddd;
	border-top: 2px solid #ddd;
	border-bottom: 2px solid #ddd;
	box-sizing: border-box;
	cursor: pointer;
	font-size: 1.125em;
	line-height: 1.5em;
	text-align: center;
}

.board-tools .sort {
	float: right;
	margin-top: 1.1875em;
}

.board-tools .sort .sort-option {
	padding-right: 0.44444em;
	display: inline-block;
	color: #999999;
	font-size: 1.125em;
	word-break: keep-all;
}

.board-tools .sort .sort-option.current {
	color: #1e1d1d;
}

.board-tools .sort .sort-option + .sort-option {
	padding-left: 0.66667em;
	position: relative;
}

.board-tools .sort .sort-option + .sort-option:before {
	content: " ";
	position: absolute;
	top: 50%;
	left: 0;
	display: block;
	margin-top: -0.5em;
	width: 2px;
	height: 15px;
	background-color: #dddddd;
}

.board-tools .sort .sort-option + .sort-option::before {
	margin-top: -7px;
}

@media screen and (max-width:767px) {
	.board-tools {
		padding-left: 0;
		padding-right: 0;
	}

	.board-tools .search-form .search-label {
		display: none;
	}

	.board-tools .search-form .search-group {
		width: 250px;
	}

}

@media screen and (max-width:441px) {
	.board-tools .search-form {
		float: none;
	}

	.board-tools .search-form .search-group {
		width: 100%;
	}

	.board-tools .sort {
		float: none;
		text-align: right;
	}

}

.btn-board-toggle {
	display: inline-block;
	padding: 5px 29px 6px 10px;
